{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.cjs",
    "**/*.mjs",
    "../prisma/generator/**/*.ts",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "./node_modules",
    "../node_modules",
  ]
}
